; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="2D Platformer Template"
config/description="Template for 2d platforming games. 
Includes:
- Character
- State machine
- Menus
- Save and load system
- Scene manager
- Data templates"
config/tags=PackedStringArray("2d", "template")
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/show_image=false
config/icon="res://common/art/meta/icon.svg"

[audio]

buses/default_bus_layout="res://common/audio/default_bus_layout.tres"

[autoload]

SaveLoad="*res://autoloads/save_load_game.gd"
Scenes="*res://autoloads/scene_manager/scene_manager.tscn"
Audio="*res://autoloads/audio_manager.gd"
Events="*res://autoloads/events.gd"
GameData="*res://autoloads/data/game_data.gd"
PlayerData="*res://autoloads/data/player_data.gd"

[display]

window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://autoloads/": "gray",
"res://common/": "red",
"res://scenes/": "blue",
"res://scenes/_common/": "red",
"res://scenes/main/": "gray"
}

[filesystem]

import/blender/enabled=false

